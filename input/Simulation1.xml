<?xml version="1.0"?>
<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../src/xsd/Simulation.xsd">
    <end_time>0.0005</end_time>
    <delta_t>0.00005</delta_t>
    <sigma>1</sigma> 
    <epsilon>5</epsilon>
    <cutoff>3.0</cutoff>
    <domain>
        <x>150</x>
        <y>80</y>
        <z>0</z>
    </domain>
    <boundaries>
        <xLeft>Reflecting</xLeft>
        <xRight>Reflecting</xRight>
        <yBottom>Reflecting</yBottom>
        <yTop>Reflecting</yTop>
        <zFront>Reflecting</zFront>
        <zBack>Reflecting</zBack>
    </boundaries>
    <writeFrequency>10</writeFrequency>
    <baseName>outputVTK/</baseName>
    <file_name>eingabe-sphere.txt</file_name>
</simulation>